<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sineva.slam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".com.sineva.slam.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".com.sineva.slam.activity.GuideActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.QuestionActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.WelcomeActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.BasicSettingActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.MoveControlActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.CreateMapActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.LocalMapActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.SetOrderActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.SelectSceneActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.SlamActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.VoiceRecognizeActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.AboutUsActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.HeadSettingActivity"></activity>
        <activity android:name=".com.sineva.slam.activity.StartGuideActivity"></activity>
    </application>

</manifest>